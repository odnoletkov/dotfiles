{
  ".git/|.git|.projections.json": {
    ".projections.json": {
      "type": "projections",
      "template": [
        "{",
        "  \"*\": {",
        "    \"\": \"\"",
        "  }",
        "}"
      ]
    }
  },
  "*.xcodeproj/project.pbxproj": {
    "*.xcodeproj/project.pbxproj": {
      "type": "project",
      "make": "xcodeproj-format %"
    },
    "*.xcscheme": {
      "type": "scheme"
    },
    "*.h": {
      "alternate": "{}.m",
      "type": "header"
    },
    "*.m": {
      "alternate": "{}.h",
      "type": "source"
    },
    "*.swift": {
      "type": "source",
      "make": "xcodebuild-auto -quiet"
    },
    "*": {
      "start": "xed ."
    }
  },
  "Package.swift": {
    "Package.swift": {
      "type": "package"
    },
    "Sources/*.swift": {
      "type": "source"
    },
    "Tests/*.swift": {
      "type": "test"
    },
    "*.swift": {
      "make": "swift build",
      "dispatch": "swift test"
    },
    "*": {
      "start": "xed ."
    }
  }
}
