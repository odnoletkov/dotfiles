setlocal nowrap
setlocal cursorline

nnoremap <silent> <buffer> <Leader>p :Pick<CR>
nnoremap <silent> <buffer> <Leader>s :Squash<CR>
nnoremap <silent> <buffer> <Leader>e :Edit<CR>
nnoremap <silent> <buffer> <Leader>r :Reword<CR>
nnoremap <silent> <buffer> <Leader>f :Fixup<CR>
nnoremap <silent> <buffer> <Leader>b obreak<ESC>0
nnoremap <silent> <buffer> <Leader>x oexec 
